<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.javafxController.GamesController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label fx:id="welcomeText"/>
    <Button text="y yo le dije !" onAction="#onHelloButtonClick"/>

    <TableView fx:id="juegoTabla" prefHeight="200.0" prefWidth="200.0">
        <columns>
            <TableColumn fx:id="juegoId" text="Id"/>
            <TableColumn fx:id="juegoNombre" text="Nombre"/>
            <TableColumn fx:id="juegoPegi" text="Pegi"/>
            <TableColumn fx:id="juegoPlataforma" text="Plataforma"/>
            <TableColumn fx:id="juegoCategoria" text="Categoria"/>
        </columns>
    </TableView>
</VBox>