<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.javafxController.GamesController">
    <padding>
        <Insets  bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <TableView fx:id="juegoTabla" prefHeight="200.0" prefWidth="200.0">
    <columns>
        <TableColumn fx:id="juegoId" prefWidth="75.0" text="Id" />
        <TableColumn fx:id="juegoNombre" prefWidth="75.0" text="Nombre" />
        <TableColumn fx:id="juegoPegi" prefWidth="75.0" text="Pegi" />
        <TableColumn fx:id="juegoPlataforma" prefWidth="75.0" text="Plataforma" />
        <TableColumn fx:id="juegoCategoria" prefWidth="75.0" text="Categoria" />

    </columns>
    </TableView>

    <Label fx:id="welcomeText"/>
    <Button text="Hello!" />

</VBox>